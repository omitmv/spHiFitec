{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/sendemail",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/notfound",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "mimeTypes": {
    ".json": "text/json",
    ".svg": "image/svg+xml"
  }
}
