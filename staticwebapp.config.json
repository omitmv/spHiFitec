{
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/sendemail",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https://www.hi-fitec.com.br; img-src 'self' https://www.hi-fitec.com.br; style-src 'self'; script-src 'self'"
  },
  "mimeTypes": {
    ".json": "text/json",
    ".svg": "image/svg+xml"
  }
}
